{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "omnishare/__pycache__": true
    },
    "hide-files.files": [
        "omnishare/__pycache__"
    ],
    "markdownlint.config": {
        "MD033": {
            "allowed_elements": [
                "iframe",
                "a",
                "h1",
                "p",
                "img",
            ]
        }
    }
}
